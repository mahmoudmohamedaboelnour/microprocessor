

#ifndef ROBOT_H_
#define ROBOT_H_
.ORG 0X600



STOP:

RET
GO_FORWARD :
 CBI PORTB,PIN0 // SET PIN0 IN PORTA
 SBI PORTB,PIN1 // CLEAR PIN1 IN PORTA
 
 SBI PORTB,PIN2 
 CBI PORTB,PIN3
RET






GO_BACK :

   SBI PORTB,PIN0 // SET PIN0 IN PORTA
 CBI PORTB,PIN1 // CLEAR PIN1 IN PORTA
 
 CBI PORTB,PIN2 
 SBI PORTB,PIN3

RET




GO_LEFT:
 SBI PORTB,PIN0 // SET PIN0 IN PORTA
 CBI PORTB,PIN1 // CLEAR PIN1 IN PORTA
 SBI PORTB,PIN2 
 CBI PORTB,PIN3


RET
GO_RIGHIT:
  CBI PORTB,PIN0 // SET PIN0 IN PORTA
 SBI PORTB,PIN1 // CLEAR PIN1 IN PORTA
 CBI PORTB,PIN2 
 SBI PORTB,PIN3


RET
#endif

